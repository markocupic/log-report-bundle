{
  "name": "markocupic/log-report-bundle",
  "type": "contao-bundle",
  "description": "Contao 4 log-report bundle",
  "license": "LGPL-3.0+",
  "authors": [
    {
      "name": "Marko Cupic",
      "homepage": "https://github.com/markocupic"
    }
  ],
  "require": {
    "contao/core-bundle": ">=4.3"
  },
  "autoload": {
    "psr-4": {
      "Markocupic\\LogReportBundle\\": "src/"
    },
    "classmap": [
      "src/Resources/contao"
    ],
    "exclude-from-classmap": [
      "src/Resources/contao/config",
      "src/Resources/contao/dca",
      "src/Resources/contao/languages"
    ]
  },
  "extra": {
    "contao-manager-plugin": "Markocupic\\LogReportBundle\\ContaoManager\\Plugin"
  }
}